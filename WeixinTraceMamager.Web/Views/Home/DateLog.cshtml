@model WeixinTraceMamager.Web.Models.Home_DateLogVD

@{
    ViewBag.Title = "DataLog";
}

<h2>日志 - @Model.Date</h2>

<table class="table">
    <tr>
        <th>编号</th>
        <th>行号</th>
        <th>标题</th>
        <th>类型</th>
        <th>是否异常</th>
        <th>内容</th>
    </tr>
    @{var i = 0;}
@foreach (var item in Model.WeixinTraceItemList) {
    <tr>
        <td>
            @(++i)
        </td>
        <td>@item.Line</td>
        <td>@item.Title</td>
        <td>@item.weixinTraceType</td>
        <td>@item.IsException</td>
        <td>@item.Result.ToString()</td>
    </tr>
}

</table>
